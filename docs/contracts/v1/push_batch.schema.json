{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PushBatch",
  "type": "object",
  "required": ["contract_version", "project", "source", "batch_id", "items"],
  "properties": {
    "contract_version": {
      "type": "string",
      "enum": ["1.0"]
    },
    "project": {
      "type": "string",
      "minLength": 1
    },
    "source": {
      "type": "string",
      "minLength": 1
    },
    "batch_id": {
      "type": "string",
      "minLength": 1
    },
    "items": {
      "type": "array",
      "items": {
        "$ref": "./push_item.schema.json"
      }
    }
  },
  "additionalProperties": false
}
